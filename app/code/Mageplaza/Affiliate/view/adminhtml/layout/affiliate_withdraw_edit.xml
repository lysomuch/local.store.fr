<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Mageplaza\Affiliate\Block\Adminhtml\Withdraw\Edit" name="affiliate_withdraw_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Mageplaza\Affiliate\Block\Adminhtml\Withdraw\Edit\Tabs" name="affiliate_withdraw_tabs">
                <block class="Mageplaza\Affiliate\Block\Adminhtml\Withdraw\Edit\Tab\Withdraw" name="affiliate_withdraw_edit_tab_withdraw"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">withdraw</argument>
                    <argument name="block" xsi:type="string">affiliate_withdraw_edit_tab_withdraw</argument>
                </action>
            </block>
        </referenceContainer>
    </body>
</page>
