<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" layout="1column">
	<body>
	<referenceBlock name="header.links">
		<block class="Magento\Framework\View\Element\Html\Link\Current" name="Timer">
			<arguments>
				<argument name="label" xsi:type="helper" helper="Bss\TimeCountdown\Helper\ModuleConfig::getLabelKey">
					<param name="name">value</param>
				</argument>
				<argument name="path" xsi:type="helper" helper="Bss\TimeCountdown\Helper\ModuleConfig::getUrlKey">
					<param name="name">value</param>
				</argument>
			</arguments>
		</block>
	</referenceBlock>
	</body>
</page>